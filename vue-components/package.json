{
  "name": "trame-xterm",
  "version": "0.0.0",
  "description": "Vue component to expose xterm.js",
  "repository": "Kitware/trame-xterm",
  "main": "./dist/trame-xterm.umd.js",
  "unpkg": "./dist/trame-xterm.umd.js",
  "module": "./dist/trame-xterm.mjs",
  "exports": {
    ".": {
      "import": "./dist/trame-xterm.mjs",
      "require": "./dist/trame-xterm.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore --ignore-pattern public",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "xterm": "^5.1.0",
    "xterm-theme": "1.1.0",
    "xterm-addon-fit": "^0.7.0"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || >=3.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^2.7.1",
    "semantic-release": "19.0.2",
    "vite": "^4.1.0",
    "vue": "^3.0.0"
  },
  "author": "Kitware Inc",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ]
